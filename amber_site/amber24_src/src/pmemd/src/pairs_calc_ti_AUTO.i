    end if
  end do

  ti_img_frc(2, 1, img_i) = ti_img_frc(2, 1, img_i) - dumx
  ti_img_frc(2, 2, img_i) = ti_img_frc(2, 2, img_i) - dumy
  ti_img_frc(2, 3, img_i) = ti_img_frc(2, 3, img_i) - dumz

  ipairs_sublst(ee_eval_cnt + full_eval_cnt + 1) = saved_pairlist_val

