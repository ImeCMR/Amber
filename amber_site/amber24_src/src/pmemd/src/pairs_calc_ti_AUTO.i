        f6 = 1.0d0/(scalpha*(1.0d0-vdw_weight)+r6*ti_sigma6(ic))
    else
        f6 = 1.0d0/(scalpha*(1.0d0-ti_item_weights(1,1))+r6*ti_sigma6(ic))
