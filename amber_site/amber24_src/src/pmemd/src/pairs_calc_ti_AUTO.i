        f6 = 1.0d0/(scalpha*(1.0d0-vdw_weight)+r6*ti_sigma6(ic))
    else
        f6 = 1.0d0/(scalpha*(1.0d0-ti_item_weights(1,2))+r6*ti_sigma6(ic))
    end if
    f12 = f6 * f6


    ti_pot_vdw_stk(2) = ti_pot_vdw_stk(2) + &
      ti_foureps(ic) * ( f12 - f6 ) 

    sc_dvdl_vdw_stk(2) = sc_dvdl_vdw_stk(2) + &
      ti_foureps(ic)*scalpha*vdw_dweight*f12*(2.0d0*f6-1.0d0) 

    df = df + vdw_weight * ti_foureps(ic) * delr2 * delr2 * f12 * &
      ti_sigma6(ic) * (12.0d0 * f6 - 6.0d0)

          do bar_i = 1, bar_states
             bar_cont(bar_i) = bar_cont(bar_i) - &
               ti_foureps(ic) * ( f12 - f6 ) * vdw_weight
          end do
          do bar_i = 1, bar_states
            if(gti_vdw_sc .eq. 1) then
              f6 = 1.0d0 / (scalpha*(1.0d0 - bar_lambda(2,bar_i,6)) + &
                r6 * ti_sigma6(ic) )
            else
              f6 = 1.0d0 / (scalpha*(1.0d0 - bar_lambda(2,bar_i,1)) + &
                r6 * ti_sigma6(ic) )
            end if
            f12 = f6 * f6
             bar_cont(bar_i) = bar_cont(bar_i) + &
               ti_foureps(ic) * (f12 - f6) * bar_lambda(2,bar_i,6)
          end do

      dfx = delx * df
      dfy = dely * df
      dfz = delz * df

