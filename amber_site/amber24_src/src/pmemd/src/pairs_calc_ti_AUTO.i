      vxy = vxy - delx * dfy
      vxz = vxz - delx * dfz
      vyy = vyy - dely * dfy
      vyz = vyz - dely * dfz
      vzz = vzz - delz * dfz

      dumx = dumx + dfx
      dumy = dumy + dfy
      dumz = dumz + dfz

      ti_img_frc(2, 1, img_j) = &
        ti_img_frc(2, 1, img_j) + dfx
      ti_img_frc(2, 2, img_j) = &
        ti_img_frc(2, 2, img_j) + dfy
      ti_img_frc(2, 3, img_j) = &
        ti_img_frc(2, 3, img_j) + dfz
    end if
  end do

