      ic = ico(iaci + img_iac(img_j))


      cgi_cgj = cgi * img_qterm(img_j)

      ! Do the Coulomb part of the direct sum using erfc spline table: 

      delr = sqrt(delr2)
      delrinv = 1.d0 / delr

      x = dxdr * delr
      ind = int(eedtbdns_stk * x)
